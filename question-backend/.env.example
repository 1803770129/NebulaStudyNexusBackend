# 应用配置
NODE_ENV=development
PORT=3003
API_PREFIX=api

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=password
DB_DATABASE=question_manager

# JWT 配置
JWT_SECRET=your-super-secret-key-change-in-production
JWT_EXPIRES_IN=1h
JWT_REFRESH_EXPIRES_IN=7d

# 日志配置
LOG_LEVEL=debug

# github图床 配置
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxx
GITHUB_REPO=1803770129/question-images
GITHUB_BRANCH=main

# CDN 混合降级配置
# 超时阈值（毫秒）
CDN_TIMEOUT=5000
# CDN 优先级顺序（逗号分隔）
CDN_PRIORITY=statically,github,proxy
# 启用的 CDN（true/false）
CDN_ENABLE_STATICALLY=true
CDN_ENABLE_GITHUB=true
CDN_ENABLE_PROXY=true
# URL 缓存过期时间（毫秒，默认 24 小时）
CDN_CACHE_TTL=86400000
# 健康检查缓存间隔（毫秒，默认 60 秒）
CDN_HEALTH_CHECK_INTERVAL=60000
# 代理服务缓存时间（秒，默认 1 小时）
PROXY_CACHE_TTL=3600
# 代理服务是否启用图片压缩
PROXY_ENABLE_COMPRESSION=true
# 代理服务最大缓存大小（字节，默认 100MB）
PROXY_MAX_CACHE_SIZE=104857600

# 微信小程序配置
WECHAT_APPID=
WECHAT_APP_SECRET=